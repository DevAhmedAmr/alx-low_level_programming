#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
 * main - Entry point
 *
 * Description:a program that prints all possible combinations .
 * of two two -digit numbers.
 *
 * Return: 0 succsess , else failed
 */
#include <stdio.h>

int main(void)
{
int i = 0;
for (; i <= 99; i++)
{
int j = 0;
for (; j <= 99; j++)
{
if (i != j)
{
putchar(i / 10 + '0');
putchar(i % 10 + '0');
putchar(' ');
putchar(j / 10 + '0');
putchar(j % 10 + '0');
if (i != 99 || j != 98)
{
putchar(',');
putchar(' ');
}
}
}
}
putchar('\n');
return (0);
}
#include<stdio.h>

int main(void)
{
int num1=0,num2;
while(num1<=99)
{
num2=num1;
while(num2<=99)
{
if(num2!=num1)
{
putchar((num1/10)+48);
putchar((num1%10)+48);
putchar(' ');
putchar((num1/10)+48);
putchar((num1%10)+48);
if(num1!=98||num2!=99)
{
putchar(',');
putchar(' ');
}
}
num2++;
}
num1++;
}
putchar('\n');
return(0);
}